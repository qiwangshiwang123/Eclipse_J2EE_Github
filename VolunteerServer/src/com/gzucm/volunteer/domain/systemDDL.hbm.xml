<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!--数字字典表 -->

<hibernate-mapping package="com.gzucm.volunteer.domain">
	<class name="SystemDDL" table="systemDDL">
		<id name="seqID" column="seqID">
			<generator class="uuid"></generator>
		</id>
		<property name="keyword" column="keyword" />
		<property name="ddlCode" column="ddlCode" />
		<property name="ddlName" column="ddlName" />
	</class>
</hibernate-mapping>